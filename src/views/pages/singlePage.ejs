<!-- Get Post Data -->
<script>
    async function loadPostData(postId) {
        const db = firebase.firestore();
        const postRef = db.collection('posts').doc(postId);
        const postSnapshot = await postRef.get();

        if (postSnapshot.exists) {
            const postData = postSnapshot.data();
            const postTitle = document.getElementById('title');
            postTitle.innerHTML = `
                ${postData.title}
            `;
            const postTitleName = document.getElementById('titleName');
            postTitleName.innerHTML = `
                ${postData.title}
            `;
            const postAuthor = document.getElementById('author');
            postAuthor.innerHTML = `
                ${postData.author}
            `;
            const postCategory = document.getElementById('category');
            postCategory.innerHTML = `
                ${postData.category}
            `;
            const postContent = document.getElementById('content');
            postContent.innerHTML = `
                ${postData.content}
            `;
            // get image
            const myImage = document.getElementById('myImage');
            myImage.src = postData.imageURL;

            // document.getElementById('postId').value = postId;
            // document.getElementById('title').innerHTML = postData.title;
            // document.getElementById('content').value = postData.content;
            // document.getElementById('category').value = postData.category;
            // const selectedValue = postData.purpose;
            // const myImage = document.getElementById('myImage');
            // myImage.src = postData.imageURL
            console.log(postData);
        } else {
            console.error('Post not found');
            // Redirect or show an error message
        }
        }
</script>

